package classes{	import flash.events.Event;	public class CustomEvent extends Event	{		public static const CLOSE:String = "Close";		public static const CLICK:String = "Click"; 		public var slideshowObject:*;				function CustomEvent( o:*, eventType:String )		{			slideshowObject = o;			super( eventType, true ); 		}				public override function clone():Event 		{ 			return new CustomEvent ( slideshowObject, type )		}	}}